{
  "name": "lacona-timer",
  "version": "1.1.0",
  "description": "Lacona Addon to create timers and alarms",
  "main": "lib/index.js",
  "lacona": {
    "title": "Timers & Alarms",
    "description": "Create and manage basic Timers and Alarms",
    "examples": [
      "set timer for [3 minutes](duration)",
      "set alarm for [8am](date and time)",
      "set timer called [Break Time](timer title) for [25 minutes](duration)",
      "set timer called [Pick up the kids](alarm title) at [3:30pm](date and time)"
    ],
    "engine": ">=1.1.0"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "lacona",
    "lacona-addon"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-timezone.git"
  },
  "dependencies": {
    "elliptical": "^2.0.0",
    "elliptical-datetime": "^1.0.1",
    "is-running": "^2.1.0",
    "lacona-api": "^1.4.0",
    "lacona-phrases": "^1.0.1",
    "lacona-source-helpers": "^1.4.0",
    "lodash": "^4.15.0",
    "moment": "^2.15.1"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-cli": "^6.14.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015-node": "^6.1.1",
    "rimraf": "^2.5.4"
  },
  "babel": {
    "presets": [
      "es2015-node"
    ],
    "plugins": [
      "transform-async-to-generator",
      "syntax-async-functions",
      "transform-react-jsx"
    ]
  }
}
